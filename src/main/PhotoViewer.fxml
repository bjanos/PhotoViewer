<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<BorderPane maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.PhotoViewerController">
   <left>
      <VBox spacing="4.0">
         <children>
            <Label maxWidth="1.7976931348623157E308" text="%listHeader" />
            <ListView fx:id="photoListView" maxHeight="1.7976931348623157E308" prefHeight="346.0" prefWidth="200.0" BorderPane.alignment="CENTER" VBox.vgrow="ALWAYS">
               <BorderPane.margin>
                  <Insets right="8.0" />
               </BorderPane.margin>
            </ListView>
         </children>
         <BorderPane.margin>
            <Insets right="8.0" />
         </BorderPane.margin>
      </VBox>
   </left>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
   <center>
      <VBox spacing="8.0">
         <children>
            <Label maxWidth="1.7976931348623157E308" text="%photoHeader" />
            <ImageView fx:id="photoImageView" fitHeight="346.0" fitWidth="519.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER" />
            <HBox alignment="CENTER" spacing="32.0">
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="8.0">
                     <children>
                        <Label text="%shutterSpeedLabel" />
                        <TextField fx:id="shutterText" editable="false" prefWidth="50.0" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" layoutX="673.0" layoutY="10.0" spacing="8.0">
                     <children>
                        <Label text="%apertureLabel" />
                        <TextField fx:id="apertureText" editable="false" prefWidth="50.0" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" layoutX="673.0" layoutY="10.0" spacing="8.0">
                     <children>
                        <Label text="%isoLabel" />
                        <TextField fx:id="isoText" editable="false" prefWidth="50.0" />
                     </children>
                  </HBox>
                  <HBox spacing="18.0">
                     <children>
                        <Button fx:id="editButton" mnemonicParsing="false" onAction="#editClicked" text="%editMetaButton" />
                        <Button fx:id="saveButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#saveClicked" text="%saveMetaButton" visible="false" />
                     </children>
                  </HBox>
               </children>
            </HBox>
            <HBox alignment="TOP_RIGHT" layoutX="10.0" layoutY="381.0">
               <children>
                  <HBox alignment="BASELINE_LEFT" spacing="8.0">
                     <children>
                        <Label maxWidth="1.7976931348623157E308" text="%commentLabel" />
                        <TextArea fx:id="commentTextArea" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="80.0" />
                     </children>
                  </HBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
</BorderPane>
